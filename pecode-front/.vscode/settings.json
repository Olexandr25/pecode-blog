{
  "cSpell.words": ["Pecode"],
  "eslint.codeActionsOnSave.rules": null,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true
  },
  "editor.formatOnSave": true,
  "eslint.validate": ["javascript", "javascriptreact"]
}
